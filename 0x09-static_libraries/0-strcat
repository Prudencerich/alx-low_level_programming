#include "main.h"
 /**
 * _strcat - concatenates functions of two strings in a program.

 * @dest: input values of functions in a program
 *
 * @src: input value of functions in a program
 *
 * Return: NULL
 */
char *_strcat(char *dest, char *src)
{
	int i;
	int j;


	i = 0;
	while (dest[i] != '\0')
	{
		i++;
	}
	j = 0;
	while (src[j] != '\0')
	{
		dest[i] = src[j];
		i++;
		j++;
	}		


	dest[i] = '\0';
	return (dest);
}
